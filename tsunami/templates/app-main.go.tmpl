package main

import (
	"embed"
	"io/fs"

	"github.com/wavetermdev/waveterm/tsunami/app"
)

//go:embed dist/**
var distFS embed.FS

//go:embed static/**
var staticFS embed.FS

func main() {
	subDistFS, _ := fs.Sub(distFS, "dist")
	app.RegisterAssetsFS(subDistFS)
	subStaticFS, _ := fs.Sub(staticFS, "static")
	app.RegisterStaticFS(subStaticFS)
	app.RunMain()
}
